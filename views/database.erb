<div id="database-interaction">
    <textarea id="sql-input" required style="width: 500px; height: 70px"></textarea>
    <button type="button" id="enter">Enter</button>
    <div>
        <iframe id="database-output" src="/admin/database-output/" style="width: 1300px; height: 800px; border: none;">

        </iframe>
    </div>
</div>

<script>
    document.getElementById("enter").addEventListener("click", () => {
        document.getElementById("database-output").src = "database-output/" + document.getElementById("sql-input").value;
    });
</script>